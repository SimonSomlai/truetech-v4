<% content_for :title, "Single Page | Truetech - Webdesign Mechelen" %><% content_for :description, t("single.meta") %><% content_for :css do %><%=stylesheet_link_tag "application", "range-sliders" %><% end %>
<% content_for :ogtitle, t("single.ogtitle") %>
<% content_for :ogdescription, t("single.meta") %>
<% content_for :ogtype, "article" %>
<% content_for :ogimage, image_url("service-single-1-#{I18n.locale}") %>
<body class="stretched">
   <div id="wrapper" class="clearfix">
   <header id="header" class="sticky-style-2">
      <div class="container clearfix">
         <%= link_to(home_path) do %>
         <div id="logo" class="center-block text-center">
            <h1 class="main-logo center-block text-center">TrueTech</h1>
            <h2 class="logo-subtitle"><%= t "home.sub" %></h2>
         </div>
     <% end %>
      </div>
      <div id="header-wrap">
         <% content_for :menu do %> 
         <li> <%=link_to("Home", home_path) %> </li>
         <li>
            <%=link_to "#", class: "home-menu-item" do %> 
            <div>Contact</div>
            <% end %> 
         </li>
         <%= render "shared/locale" %>
         <% end %> <%=render "shared/menu" %> 
      </div>
   </header>
   <div class="container clear-bottommargin clearfix">
      <div class="heading-block center">
         <h2><i class="col-hidden-md-down i-plain color i-xlarge icon-line2-energy inline-block"></i>Single Pages</h2>
         <span><%= t "single.descr" %></span> 
      </div>
      <div class="fancy-title title-border title-center bottommargin">
         <h2><%= t "single.heading1_first" %><span> <%= t "single.heading1_span" %></span><%= t "single.heading1_second" %></h2>
      </div>
      <div class="row">
         <div class="col-md-4 col-sm-6 topmargin bottommargin">
            <div class="col_full">
               <div class="feature-box fbox-plain fbox-small fbox-dark">
                  <div class="fbox-icon"> <i class="icon-line2-speedometer"></i> </div>
                  <h3><%= t "single.benefit1" %></h3>
                  <p><%= t "single.benefit1_descr" %><a target="_blank" href="https://tools.pingdom.com/"><%= t "single.benefit1_link" %></a></p>
               </div>
            </div>
            <div class="col_full">
               <div class="feature-box fbox-plain fbox-small fbox-dark">
                  <div class="fbox-icon"> <i class="icon-line-eye"></i> </div>
                  <h3><%= t "single.benefit2" %></h3>
                  <p><%= t "single.benefit2_descr" %></p>
               </div>
            </div>
            <div class="col_full nobottommargin">
               <div class="feature-box fbox-plain fbox-small fbox-dark">
                  <div class="fbox-icon"> <i class="icon-google-plus"></i> </div>
                  <h3><%= t "single.benefit3" %></h3>
                  <p><%= t "single.benefit3_descr" %> <a target="_blank" href="http://www.seocentro.com/tools/seo/seo-analyzer.html"><%= t "single.benefit3_link" %></a></p>
               </div>
            </div>
         </div>
         <div class="col-md-4 bottommargin hidden-xs hidden-sm"> <%=lazy_image_tag("service-single-1-#{I18n.locale}", class: "center-block fadeInLeftBig animated", alt: "Single Page - Webdesign Mechelen", :data=>{:animate=> "fadeInLeftBig"}) %> </div>
         <div class="col-md-4 col-sm-6 topmargin bottommargin">
            <div class="col_full">
               <div class="feature-box fbox-plain fbox-small fbox-dark">
                  <div class="fbox-icon"> <i class="icon-phone-portrait"></i> </div>
                  <h3><%= t "single.benefit4" %></h3>
                  <p><%= t "single.benefit4_descr" %><a target="_blank" href="https://www.google.com/webmasters/tools/mobile-friendly/"><%= t "single.benefit4_link" %></a></p>
               </div>
            </div>
            <div class="col_full">
               <div class="feature-box fbox-plain fbox-small fbox-dark">
                  <div class="fbox-icon"> <i class="icon-line2-globe"></i> </div>
                  <h3><%= t "single.benefit5" %></h3>
                  <p><%= t "single.benefit5_descr" %></p>
               </div>
            </div>
            <div class="col_full nobottommargin">
               <div class="feature-box fbox-plain fbox-small fbox-dark">
                  <div class="fbox-icon"> <i class="icon-measure"></i> </div>
                  <h3><%= t "single.benefit6" %></h3>
                  <p><%= t "single.benefit6_descr" %></p>
               </div>
            </div>
         </div>
      </div>
      <div class="container cf">
         <div class="fancy-title title-border title-center bottommargin">
            <h2><%= t "single.heading2_first" %><span> <%= t "single.heading2_span" %></span></h2>
         </div>
      </div>
      <div class="container cf">
         <div class="col_one_third nobottommargin">
            <div class="feature-box fbox-plain">
               <div class="hidden-xs fbox-icon"> <a href="#"><i class="i-alt">1.</i></a> </div>
               <h3><%= t "single.process1" %></h3>
               <p><%= t "single.process1_descr" %></p>
            </div>
         </div>
         <div class="col_one_third nobottommargin">
            <div class="feature-box fbox-plain">
               <div class="hidden-xs fbox-icon"> <a href="#"><i class="i-alt">2.</i></a> </div>
               <h3><%= t "single.process2" %></h3>
               <p><%= t "single.process2_descr1" %> <span data-lightbox="gallery"><a data-lightbox="gallery-item" href="<%=image_path('wireframe.png') %>">'wireframe'</a></span><%= t "single.process2_descr2" %> <strong><%= t "single.process2_descr3" %></strong> <%= t "single.process2_descr4" %></p>
            </div>
         </div>
         <div class="col_one_third nobottommargin col_last">
            <div class="feature-box fbox-plain">
               <div class="hidden-xs fbox-icon"> <a href="#"><i class="i-alt">3.</i></a> </div>
               <h3><%= t "single.process3" %></h3>
               <p><%= t "single.process3_descr" %></p>
            </div>
         </div>
      </div>
      <div class="fancy-title title-border title-center topmargin bottommargin">
         <h2><%= t "single.heading3_first" %> <span><%= t "single.heading3_span" %></span> <%= t "single.heading3_second" %></h2>
      </div>
      <div class="pricing-box pricing-extended bottommargin clearfix">
         <div class="pricing-desc">
            <div class="pricing-title">
               <h3><i class="icon-checkbox-checked"></i> <%= t "single.options" %></h3>
            </div>
            <div class="pricing-features">
               <ul class="clearfix page-selector">
                  <li data-toggle="tooltip" data-placement="top" title="" data-original-title="<%= t 'single.responsive_descr' %>" data-item="bootstrap" class="col_one_third nomargin cf"> <i class="icon-resize-horizontal"></i><%= t "single.responsive" %></li>
                  <li data-toggle="tooltip" data-placement="top" title="" data-original-title="<%= t 'single.optimized_descr' %>" data-item="mobile" class="col_one_third nomargin cf"> <i class="icon-line2-screen-smartphone"></i><%= t 'single.optimized' %></li>
                  <li data-item="email capture" data-placement="top" title="" data-toggle="tooltip" data-original-title="<%= t 'single.email_descr' %>" class="col_one_third nomargin cf"> <i class="icon-line-mail" ></i><%= t 'single.email' %></li>
                  <li data-item="slider" class="col_one_third nomargin cf"> <i class="icon-stack3"></i><%= t 'single.images' %></li>
                  <li data-item="bg video" class="col_one_third nomargin cf"> <i class="icon-line-video"></i><%= t 'single.video' %></li>
                  <li data-item="lightbox" class="col_one_third nomargin cf" data-placement="top" title="" data-toggle="tooltip" data-original-title="<%= t 'single.lightbox_descr' %>"> <i class="icon-lightbulb"></i><%= t 'single.lightbox' %></li>
                  <li data-item="parallax" class="col_one_third nomargin cf" data-placement="top" title="" data-toggle="tooltip" data-original-title="<%= t 'single.scroll_descr' %>"> <i class="icon-line2-mouse"></i><%= t 'single.scroll' %></li>
                  <li data-item="custom" class="col_one_third nomargin cf" data-placement="top" title="" data-toggle="tooltip" data-original-title="<%= t 'single.other_descr' %>"> <i class="icon-line2-magic-wand"></i><%= t 'single.other' %></li>
               </ul>
            </div>
         </div>
         <div class="col-md-4 col-sm-6 bottommargin-sm slider-box" style="height: 161px;">
            <div class="white-section"> <label><%= t 'single.pagenum' %></label> <input class="range_01 " readonly=""> </div>
         </div>
         <div class="pricing-action-area" data-toggle="tooltip" data-placement="top" title="" data-original-title="<%= t 'single.pricemessage' %>">
            <div class="pricing-meta"> <%= t 'single.pricehead' %><i class="icon-question-sign"></i> </div>
            <div class="pricing-price"> <span class="price-unit"><%= t "single.pricevalue" %></span><span class="price price-special">79</span><span class="price-tenure"><%= t 'single.pricefooter' %></span> </div>
            <div class="pricing-action"> <a href="#contact" class="button contact-link button-3d button-xlarge btn-block nomargin"><%= t 'single.pricebutton' %></a> </div>
         </div>
      </div>
      <div id="contact" class="line"></div>
      <div class="container cf">
         <div class="fancy-title title-border title-center bottommargin">
            <h2><%= t "contact.heading1" %><span> <%= t "contact.heading1_span" %></span>!</h2>
         </div>
      </div>
      <div class="col-sm-12 col-md-12 contactform">
         <%=form_for(@contact, remote: true, :format=> 'js', :html=>{:class=> "nobottommargin", :name=> "template-contactform", :id=> "template-contactform", :multipart=> true}) do |f| %> <%=render "shared/error_messages", object: f.object %> 
         <div class="col_half"> <%=f.label :name do %><%= t "contact.name" %><small>*</small> <% end %> <%=f.text_field :name, required: true, id: "template-contactform-name", :class=> "sm-form-control required" %> </div>
         <div class="col_half col_last"> <%=f.label :email do %> Email<small>*</small> <% end %> <%=f.email_field :email, required: true, id: "template-contactform-email", :class=> "required email sm-form-control" %> </div>
         <div class="clear"></div>
         <div class="col_half"> <%=f.label :phone, t("contact.phone") %> <%=f.text_field :phone, id: "template-contactform-phone", :class=> "sm-form-control" %> </div>
         <div class="col_half col_last"> <%=f.label :subject, t("contact.subject")  %> <%=f.text_field :subject, id: "template-contactform-options", readonly: true, :class=> "sm-form-control required" %> </div>
         <div class="clear"></div>
         <div class="col_full"> <%=f.label :message do %> <%= t("contact.message") %><small>*</small> <% end %> <%=f.text_area :message, id: "template-contactform-message", :class=> "required sm-form-control", rows: 6, cols: 30 %> </div>
         <div class="col_full"> <%=f.label :file, t("contact.file")  %> <%=f.file_field :file, id: "template-contactform-file", :class=> "sm-form-control" %> </div>
         <div class="col_full hidden"> <%=f.text_field :botcheck, id: "template-contactform-botcheck", :class=> "sm-form-control" %> </div>
         <div class="col_full"> <%=f.submit t("contact.send") , id: "template-contactform-submit", class: "button button-3d nomargin" %> </div>
         <% end %> 
      </div>
   </div>
   <%=render "shared/footer" %> 
   <div id="gotoTop" class="icon-angle-up"></div>
   <% content_for :js do %> <%=javascript_include_tag "application", "jquery.remotipart", "rangeslider.min" %> 
   <script>
   autoScroll(); 
   sliders();
   showOptions(); 
   </script> 
   <% if I18n.locale == :en %>
   <script>
   selectionPickerEn(); 
   </script>
   <% else %>
   <script>
   selectionPicker();
   </script>
   <% end %>
   <% end %>
</body>